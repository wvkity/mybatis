<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="-Infinity" prefWidth="-Infinity" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wkit.lost.mybatis.generator.ui.CustomMadeSelectTableController">
    <VBox prefHeight="580.0" prefWidth="962.0">
        <AnchorPane prefHeight="85.0" prefWidth="960.0">
