logging:
  config: classpath:log4j2.xml

server:
  port: 8888
  servlet:
    context-path: /
spring:
  datasource:
    # MYSQL
    driver-class-name: $!DRIVER_CLASS_NAME
    url: $!JDBC_URL
    username: $!USER_NAME
    password: "$!PASSWORD"
    type: com.zaxxer.hikari.HikariDataSource
    hikari:
      minimum-idle: 8
      maximum-pool-size: 20
      auto-commit: true
      idle-timeout: 30000
      pool-name: DatebookHikariCP
      max-lifetime: 1800000
      connection-timeout: 300000
